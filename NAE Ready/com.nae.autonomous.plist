<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.nae.autonomous</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>__NAE_READY_PATH__/nae_autonomous_wrapper.sh</string>
    </array>
    
    <key>WorkingDirectory</key>
    <string>__NAE_READY_PATH__</string>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
        <key>Crashed</key>
        <true/>
    </dict>
    
    <key>StartInterval</key>
    <integer>300</integer>
    
    <key>StandardOutPath</key>
    <string>__NAE_LOGS_PATH__/nae_autonomous.out</string>
    
    <key>StandardErrorPath</key>
    <string>__NAE_LOGS_PATH__/nae_autonomous.err</string>
    
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
        <key>PYTHONUNBUFFERED</key>
        <string>1</string>
    </dict>
    
    <key>ThrottleInterval</key>
    <integer>10</integer>
</dict>
</plist>

